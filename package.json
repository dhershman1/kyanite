{
  "name": "kyanite",
  "version": "0.10.2",
  "description": "A small library of pure functional utilities to make life easier and data better",
  "main": "src/common.js",
  "module": "src/index.js",
  "jsdelivr": "dist/kyanite.min.js",
  "directories": {
    "test": "tests",
    "lib": "src"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@killparadise/docdash": "github:dhershman1/docdash",
    "coveralls": "3.0.2",
    "esm": "3.0.84",
    "globby": "8.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "4.0.1",
    "npm-run-all": "4.1.3",
    "nyc": "13.1.0",
    "rollup": "0.67.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-cleanup": "3.0.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-terser": "3.0.0",
    "standard": "12.0.1",
    "tap-spec": "5.0.0",
    "tape": "4.9.1"
  },
  "scripts": {
    "test": "tape -r esm tests/**/*.js | tap-spec",
    "lint": "standard src/**/*.js",
    "build": "rollup -c",
    "create": "node scripts/create-export.js",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "scripts": "npm-run-all create docs build",
    "test:cov": "nyc npm test",
    "check-cov": "nyc check-coverage --lines 95 --functions 100 --branches 95",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "cov": "nyc --reporter=html npm test",
    "prepack": "npm-run-all --parallel create docs lint test --serial build"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "tests/**",
      "src/_internals",
      "dist/**"
    ]
  },
  "standard": {
    "ignore": [
      "dist/*",
      "docs.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/kyanite"
  },
  "keywords": [
    "kyanite",
    "functional",
    "library",
    "pure",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility",
    "single type"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/kyanite/issues",
    "email": "dustinh17@gmail.com"
  },
  "homepage": "https://dusty.codes/documentation/kyanite"
}
