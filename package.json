{
  "name": "kyanite",
  "version": "0.3.0",
  "description": "A small library of pure functional utilities to make life easier and data better",
  "main": "dist/kyanite.min.js",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "coveralls": "3.0.1",
    "del": "3.0.0",
    "esm": "3.0.36",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "nyc": "11.8.0",
    "rollup": "0.59.2",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-cleanup": "3.0.0-beta.1",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-uglify": "4.0.0",
    "standard": "11.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.9.0"
  },
  "scripts": {
    "prepack": "rollup -c rollup.split.js && node scripts/create-export.js && rollup -c && node scripts/docs.js",
    "build": "rollup -c",
    "split": "rollup -c rollup.split.js",
    "scripts": "node scripts/create-export.js && node scripts/docs.js",
    "stats": "rollup -c rollup.split.js && node scripts/clean.js",
    "clean": "node scripts/clean.js",
    "postpublish": "npm run clean",
    "test": "tape -r esm tests/**/*.js | tap-spec",
    "test:cov": "nyc npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "cov": "nyc --reporter=html npm test",
    "lint": "standard src/*.js"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "standard": {
    "ignore": [
      "dist/*",
      "docs.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/kyanite"
  },
  "keywords": [
    "kyanite",
    "functional",
    "library",
    "pure",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility",
    "single type"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/kyanite/issues"
  },
  "homepage": "https://github.com/dhershman1/kyanite#readme"
}
